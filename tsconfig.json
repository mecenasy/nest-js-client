{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
    ],
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "rootDir": "./",
  },
  "include": [
    "./",
  ],
}